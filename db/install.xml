<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/quizport/db" VERSION="20131116" COMMENT="XMLDB file for Moodle mod/quizport"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="quizport" COMMENT="details of QuizPort activities" NEXT="quizport_units">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="course" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="name" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="timecreated"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="quizport_units" COMMENT="details of QuizPort units" PREVIOUS="quizport" NEXT="quizport_unit_grades">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="parenttype"/>
        <FIELD NAME="parenttype" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="0 : quizport : the “parentid” is an “id” in the “quizport” table 1 : blocks : the “parentid” is an “id” in the “blocks_instance” table" PREVIOUS="id" NEXT="parentid"/>
        <FIELD NAME="parentid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the “id” of the parent record, in the “QuizPort” or “block_instance” tables, with which this record is associated" PREVIOUS="parenttype" NEXT="entrycm"/>
        <FIELD NAME="entrycm" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the course module activity that must be completed before this QuizPort can be attempted" PREVIOUS="parentid" NEXT="entrygrade"/>
        <FIELD NAME="entrygrade" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="100" SEQUENCE="false" ENUM="false" COMMENT="the minimum grade required for entrycm" PREVIOUS="entrycm" NEXT="entrypage"/>
        <FIELD NAME="entrypage" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="0 : no : an intro page will not be shown before the QuizPort unit is begun 1 : yes : an intro page will be shown before the QuizPort unit is begun" PREVIOUS="entrygrade" NEXT="entrytext"/>
        <FIELD NAME="entrytext" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="the text, if any, that is to appear on the intro page, which is shown before a QuizPort unit is begun" PREVIOUS="entrypage" NEXT="entryoptions"/>
        <FIELD NAME="entryoptions" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="switches to define which elements are shown on the entry page" PREVIOUS="entrytext" NEXT="exitpage"/>
        <FIELD NAME="exitpage" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="entryoptions" NEXT="exittext"/>
        <FIELD NAME="exittext" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="text displayed on exit page" PREVIOUS="exitpage" NEXT="exitoptions"/>
        <FIELD NAME="exitoptions" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="switches to define which elements are shown on the exit page" PREVIOUS="exittext" NEXT="exitcm"/>
        <FIELD NAME="exitcm" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the course module activity to be shown when this QuizPort is completed" PREVIOUS="exitoptions" NEXT="exitgrade"/>
        <FIELD NAME="exitgrade" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the minimum grade required to show link to exitcm" PREVIOUS="exitcm" NEXT="showpopup"/>
        <FIELD NAME="showpopup" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="0 : no : the QuizPort unit will be shown in the Moodle window 1 : yes : the QuizPort unit will be shown in a new window" PREVIOUS="exitgrade" NEXT="popupoptions"/>
        <FIELD NAME="popupoptions" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="a comma-delimited list of attributes for the popup window, if any resizable=1,scrollbars=1,directories=1,location=1,menubar=1,toolbar=1,status=1,height=450,width=620" PREVIOUS="showpopup" NEXT="timeopen"/>
        <FIELD NAME="timeopen" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the day and time, as a UNIX date stamp, at which the unit becomes available to the students" PREVIOUS="popupoptions" NEXT="timeclose"/>
        <FIELD NAME="timeclose" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the day and time, as a UNIX date stamp, at which the unit ceases to be available to the students" PREVIOUS="timeopen" NEXT="timelimit"/>
        <FIELD NAME="timelimit" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the time, as a UNIX date stamp, within which a student must complete the unit" PREVIOUS="timeclose" NEXT="delay1"/>
        <FIELD NAME="delay1" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the minimum time delay, as a UNIX date stamp, between the first and second attempts" PREVIOUS="timelimit" NEXT="delay2"/>
        <FIELD NAME="delay2" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the minimum time delay, as a UNIX date stamp, between attempts after the second attempt" PREVIOUS="delay1" NEXT="password"/>
        <FIELD NAME="password" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="the password which must be entered by students before they can attempt the unit" PREVIOUS="delay2" NEXT="subnet"/>
        <FIELD NAME="subnet" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="a comma separated list of full or partial IP addresses that defines which computers can access the QuizPort unit" PREVIOUS="password" NEXT="allowresume"/>
        <FIELD NAME="allowresume" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false" COMMENT="0 : no : attempts at this unit may not be resumed 1 : yes : attempts at this unit may be resumed" PREVIOUS="subnet" NEXT="allowfreeaccess"/>
        <FIELD NAME="allowfreeaccess" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="allowresume" NEXT="attemptlimit"/>
        <FIELD NAME="attemptlimit" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="0 : unlimited number of attempts are allowed &amp;gt;0 : the maximum number of times this unit may be attempted" PREVIOUS="allowfreeaccess" NEXT="attemptgrademethod"/>
        <FIELD NAME="attemptgrademethod" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="attemptlimit" NEXT="grademethod"/>
        <FIELD NAME="grademethod" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false" COMMENT="1 : highest : the score of the highest-scoring attempt at this unit by this user 2 : average : the average score of all attempts at this unit by this user 3 : first : the score of the first attempt at this unit by this user 4 : last : the score of the last attempt at this unit by this user" PREVIOUS="attemptgrademethod" NEXT="gradeignore"/>
        <FIELD NAME="gradeignore" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="grademethod" NEXT="gradelimit"/>
        <FIELD NAME="gradelimit" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="100" SEQUENCE="false" ENUM="false" COMMENT="the maximum grade for an attempt at this unit grades from attempts are assumed to be percentages and are scaled to this score" PREVIOUS="gradeignore" NEXT="gradeweighting"/>
        <FIELD NAME="gradeweighting" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="100" SEQUENCE="false" ENUM="false" COMMENT="the weighting of the grade for this attempt at the unit toward the grade for QuizPort activity &amp;gt;0 : the weighting as a percentage 0 : no contribution -1 : equal : equal weighting as other attempts (at this QuizPort unit) with this setting" PREVIOUS="gradelimit"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_units"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="parenttype-parentid" UNIQUE="true" FIELDS="parenttype, parentid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="quizport_unit_grades" COMMENT="details of QuizPort activity grades" PREVIOUS="quizport_units" NEXT="quizport_unit_attempts">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="parenttype"/>
        <FIELD NAME="parenttype" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="0 : quizport : the “parentid” is an “id” in the “quizport” table 1 : blocks : the “parentid” is an “id” in the “blocks_instance” table" PREVIOUS="id" NEXT="parentid"/>
        <FIELD NAME="parentid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the “id” of the parent record, in the “quizport” or “block_instance” tables, with which this record is associated" PREVIOUS="parenttype" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the “id” of the “user” to which this grade is associated" PREVIOUS="parentid" NEXT="grade"/>
        <FIELD NAME="grade" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the grade for this user for this quizport activity" PREVIOUS="userid" NEXT="status"/>
        <FIELD NAME="status" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the current status of this unit grade 1 : in progress : end of unit has not been reached 2 : timed out : never completed and no more time left 3 : abandoned : never completed and no more attempts left 4 : completed : end of unit has been reached at least once" PREVIOUS="grade" NEXT="duration"/>
        <FIELD NAME="duration" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the time this user has spent so far on this unit" PREVIOUS="status" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the time at which this record was last modified" PREVIOUS="duration"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_unit_grades" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="parenttype-parentid" UNIQUE="false" FIELDS="parenttype, parentid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="quizport_unit_attempts" COMMENT="details of the grades for attempts at QuizPort units" PREVIOUS="quizport_unit_grades" NEXT="quizport_quizzes">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" ENUM="false" COMMENT="id of the table, please edit me" NEXT="unitid"/>
        <FIELD NAME="unitid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the “id” of the “quizport_units” record to which this grade is associated" PREVIOUS="id" NEXT="unumber"/>
        <FIELD NAME="unumber" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the number of this attempt by this user at  this unit" PREVIOUS="unitid" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the “id” of the “user” to which this grade is associated" PREVIOUS="unumber" NEXT="grade"/>
        <FIELD NAME="grade" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the grade for this user for this attempt at a quizport unit" PREVIOUS="userid" NEXT="status"/>
        <FIELD NAME="status" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the current status of the grade of this attempt at the unit 1 : in progress : end of unit has not been reached 2 : timed out : never completed and no more time left 3 : abandoned : never completed and no more attempts left 4 : completed : end of unit has been reached at least once" PREVIOUS="grade" NEXT="duration"/>
        <FIELD NAME="duration" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the time this user has spent so far on this attempt at this unit" PREVIOUS="status" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the time at which this record was last modified" PREVIOUS="duration"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table, please edit me" NEXT="unitid"/>
        <KEY NAME="unitid" TYPE="foreign" FIELDS="unitid" REFTABLE="quizport_units" REFFIELDS="id" PREVIOUS="primary" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="unitid"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="userid-unitid-unumber" UNIQUE="true" FIELDS="userid, unitid, unumber"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="quizport_quizzes" COMMENT="details of QuizPort quizzes" PREVIOUS="quizport_unit_attempts" NEXT="quizport_quiz_scores">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="unitid"/>
        <FIELD NAME="unitid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the “id” of the “quizport_units” record to which this QuizPort quiz is associated" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="unitid" NEXT="sourcefile"/>
        <FIELD NAME="sourcefile" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="either the URL or file path (within the Files folder) of the source file for this quiz" PREVIOUS="name" NEXT="sourcetype"/>
        <FIELD NAME="sourcetype" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="the type of the sourcefile file either “html” or the name of the quiz creator software, e.g. QuizPort, qedoc, examview each type has its own subfolder beneath the “mod/quizport/types” folder" PREVIOUS="sourcefile" NEXT="sourcelocation"/>
        <FIELD NAME="sourcelocation" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="sourcetype" NEXT="configfile"/>
        <FIELD NAME="configfile" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="sourcelocation" NEXT="configlocation"/>
        <FIELD NAME="configlocation" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="configfile" NEXT="outputformat"/>
        <FIELD NAME="outputformat" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="the required output format for this quiz" PREVIOUS="configlocation" NEXT="navigation"/>
        <FIELD NAME="navigation" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the navigation used for quizzes in this QuizPort unit 1 : bar : Moodle navigation bar 2 : frame : Moodle navigation frame 3 : iframe : embedded IFRAME 4 : original : quiz’s original navigation 5 : giveup : a single “Give Up” button 6 : none" PREVIOUS="outputformat" NEXT="title"/>
        <FIELD NAME="title" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="3" SEQUENCE="false" ENUM="false" PREVIOUS="navigation" NEXT="stopbutton"/>
        <FIELD NAME="stopbutton" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="title" NEXT="stoptext"/>
        <FIELD NAME="stoptext" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="stopbutton" NEXT="allowpaste"/>
        <FIELD NAME="allowpaste" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="stoptext" NEXT="usefilters"/>
        <FIELD NAME="usefilters" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="allowpaste" NEXT="useglossary"/>
        <FIELD NAME="useglossary" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="usefilters" NEXT="usemediafilter"/>
        <FIELD NAME="usemediafilter" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="useglossary" NEXT="studentfeedback"/>
        <FIELD NAME="studentfeedback" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="usemediafilter" NEXT="studentfeedbackurl"/>
        <FIELD NAME="studentfeedbackurl" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="studentfeedback" NEXT="timeopen"/>
        <FIELD NAME="timeopen" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="studentfeedbackurl" NEXT="timeclose"/>
        <FIELD NAME="timeclose" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="timeopen" NEXT="timelimit"/>
        <FIELD NAME="timelimit" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="-1" SEQUENCE="false" ENUM="false" PREVIOUS="timeclose" NEXT="delay1"/>
        <FIELD NAME="delay1" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="timelimit" NEXT="delay2"/>
        <FIELD NAME="delay2" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="delay1" NEXT="delay3"/>
        <FIELD NAME="delay3" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="2" SEQUENCE="false" ENUM="false" PREVIOUS="delay2" NEXT="password"/>
        <FIELD NAME="password" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="delay3" NEXT="subnet"/>
        <FIELD NAME="subnet" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="password" NEXT="allowresume"/>
        <FIELD NAME="allowresume" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="subnet" NEXT="reviewoptions"/>
        <FIELD NAME="reviewoptions" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="allowresume" NEXT="attemptlimit"/>
        <FIELD NAME="attemptlimit" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="reviewoptions" NEXT="scoremethod"/>
        <FIELD NAME="scoremethod" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false" COMMENT="1 : highest : the score of the highest-scoring attempt at this quiz by this user 2 : average : the average score of all attempts at this quiz by this user 3 : first : the score of the first attempt at this quiz by this user 4 : last : the score of the last attempt at this quiz by this user" PREVIOUS="attemptlimit" NEXT="scoreignore"/>
        <FIELD NAME="scoreignore" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="scoremethod" NEXT="scorelimit"/>
        <FIELD NAME="scorelimit" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="100" SEQUENCE="false" ENUM="false" COMMENT="the maximum score for this quiz scores from quizzes are assumed to be percentages and are scaled to this score" PREVIOUS="scoreignore" NEXT="scoreweighting"/>
        <FIELD NAME="scoreweighting" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="-1" SEQUENCE="false" ENUM="false" COMMENT="the weighting of this quiz score toward the grade for this attempt at the unit &amp;gt;0 : the weighting as a percentage 0 : no contribution -1 : equal : equal weighting as other quizzes (in this QuizPort unit) with this setting" PREVIOUS="scorelimit" NEXT="sortorder"/>
        <FIELD NAME="sortorder" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="scoreweighting" NEXT="clickreporting"/>
        <FIELD NAME="clickreporting" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="sortorder" NEXT="discarddetails"/>
        <FIELD NAME="discarddetails" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="clickreporting"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_quizzes" NEXT="unitid"/>
        <KEY NAME="unitid" TYPE="foreign" FIELDS="unitid" REFTABLE="quizport_units" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="sourcelocation-sourcefile" UNIQUE="false" FIELDS="sourcelocation, sourcefile"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="quizport_quiz_scores" COMMENT="details of QuizPort quiz scores" PREVIOUS="quizport_quizzes" NEXT="quizport_quiz_attempts">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="quizid"/>
        <FIELD NAME="quizid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="unumber"/>
        <FIELD NAME="unumber" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="quizid" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="unumber" NEXT="score"/>
        <FIELD NAME="score" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="status"/>
        <FIELD NAME="status" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="score" NEXT="duration"/>
        <FIELD NAME="duration" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="status" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="duration"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_responses" NEXT="quizid"/>
        <KEY NAME="quizid" TYPE="foreign" FIELDS="quizid" REFTABLE="quizport_quizzes" REFFIELDS="id" PREVIOUS="primary" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="quizid"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="quizid-userid-unumber" UNIQUE="true" FIELDS="quizid, userid, unumber" NEXT="quizid-userid"/>
        <INDEX NAME="quizid-userid" UNIQUE="false" FIELDS="quizid, userid" PREVIOUS="quizid-userid-unumber"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="quizport_quiz_attempts" COMMENT="details of QuizPort quiz attempts" PREVIOUS="quizport_quiz_scores" NEXT="quizport_cache">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="quizid"/>
        <FIELD NAME="quizid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="quizid" NEXT="unumber"/>
        <FIELD NAME="unumber" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the number of this attempt by this user at this unit" PREVIOUS="userid" NEXT="qnumber"/>
        <FIELD NAME="qnumber" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the number of this attempt by this user at this quiz within this attempt at this unit" PREVIOUS="unumber" NEXT="status"/>
        <FIELD NAME="status" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false" COMMENT="the current status of this attempt 1 : in progress 2 : timed out 3 : abandoned 4 : completed Note that a QuizPort quiz is only considered completed when a correct answer has been found for ALL questions" PREVIOUS="qnumber" NEXT="penalties"/>
        <FIELD NAME="penalties" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the number of penalty points that this user picked on this attempt at the quiz" PREVIOUS="status" NEXT="score"/>
        <FIELD NAME="score" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the score for this attempt (so far)" PREVIOUS="penalties" NEXT="duration"/>
        <FIELD NAME="duration" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="score" NEXT="starttime"/>
        <FIELD NAME="starttime" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="start time as recorded by the client" PREVIOUS="duration" NEXT="endtime"/>
        <FIELD NAME="endtime" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="End time recorded by the client" PREVIOUS="starttime" NEXT="resumestart"/>
        <FIELD NAME="resumestart" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the most recent time at which this attempt at this quiz was resumd" PREVIOUS="endtime" NEXT="resumefinish"/>
        <FIELD NAME="resumefinish" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the most recent time at which a resumed attempt was finished" PREVIOUS="resumestart" NEXT="timestart"/>
        <FIELD NAME="timestart" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the time, according to the server, that this attempt started" PREVIOUS="resumefinish" NEXT="timefinish"/>
        <FIELD NAME="timefinish" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the time, according to the server, that this attempt finished" PREVIOUS="timestart" NEXT="clickreportid"/>
        <FIELD NAME="clickreportid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the attemptid of the first attempt in this click report chain" PREVIOUS="timefinish"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_quiz_attempts" NEXT="quizid"/>
        <KEY NAME="quizid" TYPE="foreign" FIELDS="quizid" REFTABLE="quizport_quizzes" REFFIELDS="id" PREVIOUS="primary" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="quizid"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="clickreportid" UNIQUE="false" FIELDS="clickreportid" NEXT="quizid-userid-unumber"/>
        <INDEX NAME="quizid-userid-unumber" UNIQUE="false" FIELDS="quizid, userid, unumber" PREVIOUS="clickreportid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="quizport_cache" COMMENT="cache for QuizPort quizzes" PREVIOUS="quizport_quiz_attempts" NEXT="quizport_conditions">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="quizid"/>
        <FIELD NAME="quizid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the &quot;id&quot; of the &quot;quizport_quizzes&quot; record to which this cached content is associated" PREVIOUS="id" NEXT="slasharguments"/>
        <FIELD NAME="slasharguments" TYPE="char" LENGTH="1" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="Moodle's $CFG-&amp;gt;slasharguments setting at the time the cached content was created" PREVIOUS="quizid" NEXT="quizport_bodystyles"/>
        <FIELD NAME="quizport_bodystyles" TYPE="char" LENGTH="8" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="slasharguments" NEXT="quizport_enableobfuscate"/>
        <FIELD NAME="quizport_enableobfuscate" TYPE="char" LENGTH="1" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="quizport_bodystyles" NEXT="quizport_enableswf"/>
        <FIELD NAME="quizport_enableswf" TYPE="char" LENGTH="1" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="quizport_enableobfuscate" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="quiz name at the time the cache was created" PREVIOUS="quizport_enableswf" NEXT="sourcefile"/>
        <FIELD NAME="sourcefile" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="the quiz's &quot;reference&quot; setting at the time the cached content was created" PREVIOUS="name" NEXT="sourcetype"/>
        <FIELD NAME="sourcetype" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="sourcefile" NEXT="sourcelocation"/>
        <FIELD NAME="sourcelocation" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="sourcetype" NEXT="sourcelastmodified"/>
        <FIELD NAME="sourcelastmodified" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="sourcelocation" NEXT="sourceetag"/>
        <FIELD NAME="sourceetag" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="sourcelastmodified" NEXT="configfile"/>
        <FIELD NAME="configfile" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="sourceetag" NEXT="configlocation"/>
        <FIELD NAME="configlocation" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="configfile" NEXT="configlastmodified"/>
        <FIELD NAME="configlastmodified" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="configlocation" NEXT="configetag"/>
        <FIELD NAME="configetag" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="configlastmodified" NEXT="navigation"/>
        <FIELD NAME="navigation" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the quiz's &quot;navigation&quot; setting when the cached content was created" PREVIOUS="configetag" NEXT="stopbutton"/>
        <FIELD NAME="stopbutton" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="navigation" NEXT="title"/>
        <FIELD NAME="title" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="stopbutton" NEXT="stoptext"/>
        <FIELD NAME="stoptext" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="title" NEXT="allowpaste"/>
        <FIELD NAME="allowpaste" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="stoptext" NEXT="usefilters"/>
        <FIELD NAME="usefilters" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the quiz's &quot;usefilters&quot; setting when the cached content was created" PREVIOUS="allowpaste" NEXT="useglossary"/>
        <FIELD NAME="useglossary" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the quiz's &quot;useglossary&quot; setting when the cached content was created" PREVIOUS="usefilters" NEXT="usemediafilter"/>
        <FIELD NAME="usemediafilter" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the quiz's &quot;usemediafilter&quot; setting when the cached content was created" PREVIOUS="useglossary" NEXT="studentfeedback"/>
        <FIELD NAME="studentfeedback" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the quiz's &quot;studentfeedback&quot; setting when the cached content was created" PREVIOUS="usemediafilter" NEXT="studentfeedbackurl"/>
        <FIELD NAME="studentfeedbackurl" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="the quiz's &quot;outputformaturl&quot; setting when the cached content was created" PREVIOUS="studentfeedback" NEXT="timelimit"/>
        <FIELD NAME="timelimit" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="-1" SEQUENCE="false" ENUM="false" PREVIOUS="studentfeedbackurl" NEXT="delay3"/>
        <FIELD NAME="delay3" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="-1" SEQUENCE="false" ENUM="false" PREVIOUS="timelimit" NEXT="clickreporting"/>
        <FIELD NAME="clickreporting" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="delay3" NEXT="content"/>
        <FIELD NAME="content" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="the cached content for this quiz - base64_encode(serialize($content))" PREVIOUS="clickreporting" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="the time at which this record was last modified" PREVIOUS="content" NEXT="md5key"/>
        <FIELD NAME="md5key" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="md5 (wwwroot + currenttheme + navigation)" PREVIOUS="timemodified"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_cache" NEXT="quizid"/>
        <KEY NAME="quizid" TYPE="foreign" FIELDS="quizid" REFTABLE="quizport_quizzes" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="quizid-md5key" UNIQUE="false" FIELDS="quizid, md5key" COMMENT="quizid + md5(wwwroot + currenttheme + navigation)"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="quizport_conditions" COMMENT="details of QuizPort conditions" PREVIOUS="quizport_cache" NEXT="quizport_details">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="quizid"/>
        <FIELD NAME="quizid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the “id” of the “quizport_quizzes” record to which this condition is associated" PREVIOUS="id" NEXT="groupid"/>
        <FIELD NAME="groupid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="id of group, if any, to which these conditions apply" PREVIOUS="quizid" NEXT="conditiontype"/>
        <FIELD NAME="conditiontype" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="0 : precondition: a condition that must be satisfied before a student can access this quiz 1 : postcondition : a condition that defines what happens after a student finishes this quiz" PREVIOUS="groupid" NEXT="conditionscore"/>
        <FIELD NAME="conditionscore" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the minimum score at which this condition is satisfied" PREVIOUS="conditiontype" NEXT="conditionquizid"/>
        <FIELD NAME="conditionquizid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the “id” of the “quizport_quizzes” record to which this condition refers &amp;gt;0 : a specific QuizPort quiz -1 : next : the next QuizPort quiz, if any, in this QuizPort unit according to “sortorder” -2 : previous : the previous QuizPort quiz, if any, in this QuizPort unit according to “sortorder” -3 : unseen : an unseen QuizPort quiz (i.e. no attempts) -4 : unanswered : an unanswered QuizPort quiz (i.e. no responses)  -5 : incorrect : an incorrectly answered QuizPort quiz (i.e. score" PREVIOUS="conditionscore" NEXT="sortorder"/>
        <FIELD NAME="sortorder" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="conditionquizid" NEXT="attempttype"/>
        <FIELD NAME="attempttype" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="0 : any : any attempt(s)  1 : recent : the most recent attempt(s) 2 : consecutive : any block of consecutive attempts" PREVIOUS="sortorder" NEXT="attemptcount"/>
        <FIELD NAME="attemptcount" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the number of attempts which must satisfy the score condition" PREVIOUS="attempttype" NEXT="attemptduration"/>
        <FIELD NAME="attemptduration" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="0 : no time limit &amp;gt;0 : the total time limit for the attempts which satisfy the score condition" PREVIOUS="attemptcount" NEXT="attemptdelay"/>
        <FIELD NAME="attemptdelay" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="attemptduration" NEXT="nextquizid"/>
        <FIELD NAME="nextquizid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="attemptdelay"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_conditions" NEXT="quizid"/>
        <KEY NAME="quizid" TYPE="foreign" FIELDS="quizid" REFTABLE="quizport_quizzes" REFFIELDS="id" PREVIOUS="primary" NEXT="groupid"/>
        <KEY NAME="groupid" TYPE="foreign" FIELDS="groupid" REFTABLE="groups" REFFIELDS="id" PREVIOUS="quizid"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="quizid-conditiontype-groupid" UNIQUE="false" FIELDS="quizid, conditiontype, groupid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="quizport_details" COMMENT="raw details (as XML) of QuizPort quiz attempts" PREVIOUS="quizport_conditions" NEXT="quizport_questions">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="attemptid"/>
        <FIELD NAME="attemptid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="details"/>
        <FIELD NAME="details" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="the raw XML returned from the browser regarding the attempt at a quiz" PREVIOUS="attemptid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_details" NEXT="attemptid"/>
        <KEY NAME="attemptid" TYPE="foreign" FIELDS="attemptid" REFTABLE="quizport_quiz_attempts" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="quizport_questions" COMMENT="details of questions used in QuizPort quizzes" PREVIOUS="quizport_details" NEXT="quizport_responses">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="quizid"/>
        <FIELD NAME="quizid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="a unique name for this question. If the “text”field is empty then this field is displayed as the question to the user" PREVIOUS="quizid" NEXT="md5key"/>
        <FIELD NAME="md5key" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="name" NEXT="type"/>
        <FIELD NAME="type" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the type of this question.  This field can be whatever is useful to the PHP class for this quiz type. Questions for QuizPort quizzes are given the following values: 1 : JBC 2 : JCloze 3 : JCross 4 : JMatch 5 : JMix 6 : JQuiz 7 : Rhubarb (TexToys) 8 : Sequitur (TexToys)" PREVIOUS="md5key" NEXT="text"/>
        <FIELD NAME="text" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="the “id” of the “quizport_strings” record which holds text of the question this field is used in preference to the “name” field when displaying a question" PREVIOUS="type"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_questions" NEXT="quizid"/>
        <KEY NAME="quizid" TYPE="foreign" FIELDS="quizid" REFTABLE="quizport_quizzes" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="quizid-md5key" UNIQUE="false" FIELDS="quizid, md5key"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="quizport_responses" COMMENT="details of responses in QuizPort quiz attempts" PREVIOUS="quizport_questions" NEXT="quizport_strings">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="attemptid"/>
        <FIELD NAME="attemptid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="questionid"/>
        <FIELD NAME="questionid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="attemptid" NEXT="score"/>
        <FIELD NAME="score" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="questionid" NEXT="weighting"/>
        <FIELD NAME="weighting" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="score" NEXT="hints"/>
        <FIELD NAME="hints" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="weighting" NEXT="clues"/>
        <FIELD NAME="clues" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="hints" NEXT="checks"/>
        <FIELD NAME="checks" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="clues" NEXT="correct"/>
        <FIELD NAME="correct" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="checks" NEXT="wrong"/>
        <FIELD NAME="wrong" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="correct" NEXT="ignored"/>
        <FIELD NAME="ignored" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="wrong"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_responses" NEXT="attemptid"/>
        <KEY NAME="attemptid" TYPE="foreign" FIELDS="attemptid" REFTABLE="quizport_quiz_attempts" REFFIELDS="id" PREVIOUS="primary" NEXT="questionid"/>
        <KEY NAME="questionid" TYPE="foreign" FIELDS="questionid" REFTABLE="quizport_questions" REFFIELDS="id" PREVIOUS="attemptid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="quizport_strings" COMMENT="strings used in QuizPort questions and responses" PREVIOUS="quizport_responses">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="string"/>
        <FIELD NAME="string" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="md5key"/>
        <FIELD NAME="md5key" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="string"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for quizport_strings"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="md5key" UNIQUE="false" FIELDS="md5key"/>
      </INDEXES>
    </TABLE>
  </TABLES>
